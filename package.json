  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "mysql2": "^2.1.0",
    "papaparse": "^5.1.1",
    "react": "^16.12.0",
    "react-table": "^7.0.0-rc.15",
    "sequelize": "^5.21.3"
  }
}